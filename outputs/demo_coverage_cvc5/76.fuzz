(set-logic ALL)
(declare-sort mySort ())
(define-sort-constructor mySort$int ((Int)) Int)
(define-sort-constructor mySort$bool ((Bool)) Bool)
(declare-fun x () Int)
(declare-fun y () Bool)
(declare-fun t () Int)
(declare-fun f () Int)
(assert (= x (mySort$int t)))
(declare-fun p () Int)
(assert (and (= x (mySort$int p))
    (= y (mySort$bool true))))
(assert (forall ((z Int))
    (=> (if (= z t) (= z (mySort$int y)) (= z (mySort$int f))) (= x z))))
(check-sat)
(exit)